{
  "info": {
    "name": "Backfill Tool Example Collection",
    "_postman_id": "12345-67890",
    "description": "Example collection demonstrating all features of the backfill tool",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Simple POST Request",
      "request": {
        "method": "POST",
        "url": {
          "raw": "https://jsonplaceholder.typicode.com/posts"
        },
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"title\": \"{{name}}\",\n  \"body\": \"Year: {{year}}\",\n  \"userId\": 1\n}"
        }
      }
    },
    {
      "name": "GET with Path Variable",
      "request": {
        "method": "GET",
        "url": {
          "raw": "https://jsonplaceholder.typicode.com/users/{{userId}}"
        },
        "header": []
      }
    },
    {
      "name": "GET with Query Parameters",
      "request": {
        "method": "GET",
        "url": {
          "raw": "https://jsonplaceholder.typicode.com/posts?userId={{userId}}&title={{name}}"
        },
        "header": []
      }
    },
    {
      "name": "Request with Dynamic Headers",
      "request": {
        "method": "POST",
        "url": {
          "raw": "https://httpbin.org/post"
        },
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "X-User-Name",
            "value": "{{name}}"
          },
          {
            "key": "X-User-Year",
            "value": "{{year}}"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"{{name}}\",\n  \"year\": {{year}}\n}"
        }
      }
    },
    {
      "name": "User Management Folder",
      "item": [
        {
          "name": "Create User",
          "request": {
            "method": "POST",
            "url": {
              "raw": "https://jsonplaceholder.typicode.com/users"
            },
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"{{name}}\",\n  \"email\": \"{{email}}\",\n  \"year\": {{year}}\n}"
            }
          }
        },
        {
          "name": "Update User",
          "request": {
            "method": "PUT",
            "url": {
              "raw": "https://jsonplaceholder.typicode.com/users/{{userId}}"
            },
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"{{name}}\",\n  \"email\": \"{{email}}\"\n}"
            }
          }
        },
        {
          "name": "Delete User",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "https://jsonplaceholder.typicode.com/users/{{userId}}"
            },
            "header": []
          }
        }
      ]
    },
    {
      "name": "Complex Nested Example",
      "item": [
        {
          "name": "Posts Subfolder",
          "item": [
            {
              "name": "Get Post by ID",
              "request": {
                "method": "GET",
                "url": {
                  "raw": "https://jsonplaceholder.typicode.com/posts/{{postId}}"
                },
                "header": []
              }
            },
            {
              "name": "Update Post",
              "request": {
                "method": "PATCH",
                "url": {
                  "raw": "https://jsonplaceholder.typicode.com/posts/{{postId}}"
                },
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"title\": \"{{name}}\"\n}"
                }
              }
            }
          ]
        }
      ]
    }
  ]
}
